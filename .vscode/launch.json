{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Debug Jest Server Tests",
      "type": "node",
      "request": "launch",
      "runtimeArgs": [
        "--inspect-brk",
        "${workspaceRoot}/server/node_modules/jest/bin/jest.js",
        "--detectOpenHandles",
        "--runInBand",
        "-c",
        "${workspaceRoot}/server/package.json"
      ],
      "console": "integratedTerminal",
      "internalConsoleOptions": "neverOpen",
      "port": 9229
    }
  ]
}
